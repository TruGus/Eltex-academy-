
#include <stdio.h>



int main() {
    
  unsigned int b;
  scanf("%d", &b);
 printf("Изначальное число - %d\n",b);
 printf("Результат замены третьего бита - ");
  for (int i = 0; i < 8; i++) {
      if(b & 0x80)
      {
          if(i == 5)
          {
              printf("%c", '0');
              b <<= 1;
              continue; 
          }
          printf("%c", '1');
      }
      else
      {
          if(i == 5)
          {
              printf("%c", '1');
              b <<= 1;
              continue; 
          }
         printf("%c", '0'); 
      }
    
    b <<= 1;
  }
 
  return 0;
}
