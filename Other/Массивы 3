#include <stdio.h>

#define M 3
#define N 3

int a[M][N];
int main() {
  
  int b = N - 1;
  for(int i = 0; i <= 2; i++)
  {
    for(int j = 0; j <= 2; j++)
    {
      if(j >= b)
      {
        a[i][j] = 1;
      }
      
      printf("%d", a[i][j]);
    }

    b--;
     printf("\n");
  }

  
 
  return 0;
}
